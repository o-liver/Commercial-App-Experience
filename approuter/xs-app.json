{
    "welcomeFile": "../app/eventmanager/index.htlm",
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/do/logout"
    },
    "routes": [
        {
            "source": "^/(.*)$",
            "target": "$1",
            "authenticationType": "xsuaa",
            "scope": "$XSAPPNAME.user",
            "destination": "cae-poc-dest",
            "csrfProtection": false
        }
    ]
}